# Copy this file to .env and fill in your Supabase credentials

# For Web App (Create React App uses REACT_APP_ prefix)
REACT_APP_SUPABASE_URL=your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# For Mobile App (Expo uses EXPO_PUBLIC_ prefix)
EXPO_PUBLIC_SUPABASE_URL=your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
